  "scripts": {
    "build": "NODE_ENV=production ./node_modules/.bin/webpack",
    "lint": "eslint examples src",
    "start": "node examples/server.js",
    "test": "NODE_ENV=test node ./bin/test.js",
    "test-ci": "NODE_ENV=test ./node_modules/.bin/nyc --all ./node_modules/.bin/zuul --phantom -- test/*Spec.js",
    "test-local": "NODE_ENV=test ./node_modules/.bin/zuul --local 8080 -- test/*Spec.js"
  },
  "nyc": {
    "exclude": [
      "*config*.js",
      "bin/",
      "coverage/",
      "dist/",
      "examples/",
      "node_modules/",
      "test/"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
